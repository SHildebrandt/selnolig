% 9. fb -> f-b, fh -> f-h, fk -> f-k
% ----------------------------------

 % Disable these ligatures globally.
 % I can't think of a single *German* word
 % for which these ligatures would not
 % cross a morpheme boundary.

\nolig{fb}{f|b}
\nolig{fh}{f|h}
\nolig{fk}{f|k}

 % However, there are words of *non-German* 
 % origin for which the 'fk' ligature 
 % shouldn't be suppressed. Use \keeplig macros 
 % to define such as cases.

	\keeplig{Kafka}
	\keeplig{kafka}
   \keeplig{Piefke}
	\keeplig{Safka}
	\keeplig{Potrafke}
	\keeplig{Sprafke}
	\keeplig{Shirafkan}
	\keeplig{Tirafkan}
	\keeplig{Selfkant}


% 10. fj -> f-j
% -------------

% Suppress this ligature globally
\nolig{fj}{f|j}

 % Once more, there are some words of *non-German*
 % (e.g., Nordic and Slavic) origin for which the
 % 'fk' ligature should not be suppressed. Use
 % \keeplig macros to define such cases.

	\keeplig{fjord}
	\keeplig{fjell}
	\keeplig{Prokofjew}
	\keeplig{Sufjan}% Stevens
	\keeplig{Eefje} % Dutch first name
	\keeplig{fjäll} % Tegefjäll: Swedish skiing area
	\keeplig{Astafjew} 
     % Russian author (Wiktor) and 
     % soccer player (Maksim)
	\keeplig{fjör} 
     % A bunch of Icelandic places; catches 
     % both German Isafjördur and Icelandic 
     % Ísafjörður
	\keeplig{Ísafjarðarbær} 
     % A city in Iceland
	

% 11. fff -> ff-f
% ---------------
 % Just in case there's a font that has a
 %    triple-f ligature:

\nolig{fff}{ff|f}
 % grifffest Stofffarbe Sprengstofffalle 

